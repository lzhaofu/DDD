@{
    Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>

    <title>后台管理系统</title>
    <link  href="@Url.Content("~/Content/css/sys.css")" rel="stylesheet" type="text/css" />

    <style type="text/css">
        html, body, #wrapper {
            height: 100%;
            margin: 0;
            padding: 0;
            border: none;
            background: #4d63a7;
        }

        #wrapper {
            text-align: center;
            vertical-align: middle;
        }

        td {
            border: 0;
        }
    </style>
    <script language="javascript" type="text/javascript" src="@Url.Content("~/Scripts/CallLeft.min.js")"></script>
</head>
<body>
    <table border="0" cellpadding="0" cellspacing="0" id="wrapper">
        <tr>
            <td class="center">
                <img src="@Url.Content("~/Content/images/close.jpg")" id="myarrow" onclick="leftmenu_ctrl();"  style="cursor:pointer; " class="center-span" />
            </td>
        </tr>
    </table>

</body>
</html>

